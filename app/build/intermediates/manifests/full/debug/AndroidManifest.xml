<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.hs_weingarten.haplaner"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="24"
        android:targetSdkVersion="24" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="de.hs_weingarten.haplaner.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="de.hs_weingarten.haplaner.StundenplanBearbeiten"
            android:label="@string/actionBarNameStundenPlan"
            android:parentActivityName="de.hs_weingarten.haplaner.MainActivity" >
        </activity>
        <activity
            android:name="de.hs_weingarten.haplaner.AufgabenEinstellung"
            android:label="@string/actionBarNameAufgaben"
            android:parentActivityName="de.hs_weingarten.haplaner.MainActivity" >
        </activity>
        <activity
            android:name="de.hs_weingarten.haplaner.AufgabeBearbeiten"
            android:label="@string/actionBarNameAufgaben"
            android:parentActivityName="de.hs_weingarten.haplaner.MainActivity" >
        </activity>
    </application>

</manifest>